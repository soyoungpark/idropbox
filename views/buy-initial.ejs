    <style>

    </style>

    <body>
     <div class="content content-container container-fluid">
      <div class="title container-fluid">
       <h1 class="text-primary text-center">Search Storage</h1>
     </div>

     <div class="input-container col-xs-offset-1 col-xs-10">

       <div class="row form-group">
        <label class="form-label lb-lg">Number of Boxes</label>
        <div class="input"><input type="text" class="form-control" id="quantity">
        </div>
      </div>

      <div class="row form-group">
        <label class="form-label lb-lg">Price per Box</label>
        <div class="input"><input type="text" class="form-control" id="price">
        </div>  		
      </div>

      <div class="row form-group">
        <label class="form-label lb-lg">Date of Storage</label>
        <div class="row">
         <div class="input col-xs-6"><input type="text" id="startDate">
         </div>  
         <div class="input col-xs-6"><input type="text" id="endDate">
         </div>  		
       </div>
     </div>

     <div class="row form-group">
      <label class="form-label lb-lg">Address</label>
      <div class="input"><input type="text" class="form-control" id="address">
      </div>  	
    </div>

    <div class="row form-group">
      <label class="form-label lb-lg">Zip Code</label>
      <div class="input"><input type="text" class="form-control" id="zipCode">
      </div>    
    </div>
  </div>

  <div class="submit-button">
   <button type="submit" class="btn btn-primary col-xs-offset-1 col-xs-10" id="searchButton">Search</button>
  </div>
 <!--</form>-->
</div>

